Proceso Saludo_V2
	// Version 1
	definir nombre, apellido como caracter;
	
	escribir "Por favor, indiqueme su nombre";
	leer nombre;
	
	escribir "Hola, bienvenido/a ", nombre;
	
	// Version 2
	escribir "Por favor, indiqueme su nombre y apellido";
	leer nombre, apellido;
	
	escribir "Hola, bienvenido/a ", nombre, " ", apellido;
	
FinProceso