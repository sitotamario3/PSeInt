Proceso CalculadoraSimple
	definir A, B, C, D, E como reales;
	
	escribir "Elige dos numeros para sumarlos";
	
	leer A, B;
	
	C <- A + B;
	D <- A - B;
	E <- A * B;
	
	escribir "El resultado de la suma es ", C;
	escribir "El resultado de la resta es ", D;
	escribir "El resultado de la multiplicación es ", E;
FinProceso
